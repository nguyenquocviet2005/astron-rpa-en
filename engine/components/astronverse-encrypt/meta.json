        "options": [
          {
            "label": "Overwrite原有文件",
            "value": "overwrite"
          },
          {
            "label": "创建文件副本",
            "value": "rename"
          },
          {
            "label": "取消保存操作",
            "value": "cancel"
          }
        ],
        "default": "rename",
        "level": "advanced",
        "dynamics": [
          {
            "key": "$this.exist_handle_type.show",
