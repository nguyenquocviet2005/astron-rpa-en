{
  "VerifyCode.picture_code": {
    "key": "VerifyCode.picture_code",
    "title": "通用数英Verification Code",
    "version": "1.0.0",
    "src": "astronverse.verifycode.verifycode.VerifyCode().picture_code",
    "comment": "识别浏览器中的Verification Code @{picture_pick} ，返回识别结果 @{code_result}",
    "inputList": [
      {
        "types": "Browser",
        "formType": {
          "type": "INPUT_VARIABLE_PYTHON"
        },
        "key": "browser_obj",
        "title": "Browser Object",
        "name": "browser_obj",
        "tip": "",
        "value": [
          {
            "type": "str",
            "value": ""
          }
        ],
        "required": true
      },
      {
        "types": "WebPick",
        "formType": {
          "type": "PICK",
          "params": {
            "use": "WebPick"
          }
        },
        "key": "picture_pick",
        "title": "拾取Verification Code对象",
        "name": "picture_pick",
        "tip": "",
        "required": true,
        "noInput": true
      },
      {
        "types": "PictureCodeType",
        "formType": {
          "type": "RADIO"
        },
        "key": "code_type",
        "title": "Verification Code类型",
        "name": "code_type",
        "tip": "",
        "options": [
          {
            "label": "通用数英Verification Code1-4位",
            "value": "10110"
          },
          {
            "label": "通用数英Verification Code5-8位",
            "value": "10111"
          },
          {
            "label": "通用数英Verification Code（特殊)",
            "value": "10211"
          }
        ],
        "default": "10110",
        "required": true
      },
      {
        "types": "Bool",
        "formType": {
          "type": "SWITCH",
          "params": {}
        },
        "key": "input_flag",
        "title": "YesNo填写Input框",
        "name": "input_flag",
        "tip": "",
        "options": [
          {
            "label": "Yes",
            "value": true
          },
          {
            "label": "No",
            "value": false
          }
        ],
        "default": false,
        "required": true
      },
      {
        "types": "WebPick",
        "formType": {
          "type": "PICK",
          "params": {
            "use": "WebPick"
          }
        },
        "key": "input_box",
        "title": "Input框对象",
        "name": "input_box",
        "tip": "",
        "dynamics": [
          {
            "key": "$this.input_box.show",
            "expression": "return  $this.input_flag.value == true"
          }
        ],
        "required": true,
        "noInput": true
      }
    ],
    "outputList": [
      {
        "types": "Str",
        "formType": {
          "type": "RESULT"
        },
        "key": "code_result",
        "title": "识别结果",
        "tip": ""
      }
    ],
    "icon": "captcha-text",
    "helpManual": ""
  },
  "VerifyCode.slider_code": {
    "key": "VerifyCode.slider_code",
    "title": "通用滑块Verification Code",
    "version": "1.0.0",
    "src": "astronverse.verifycode.verifycode.VerifyCode().slider_code",
    "comment": "在背景图为 @{picture_pick} 的滑块Verification Code上，拖动滑块 @{slider_pick} 至指定位置，拖动距离为 @{drag_distance}",
    "inputList": [
      {
        "types": "Browser",
        "formType": {
          "type": "INPUT_VARIABLE_PYTHON"
        },
        "key": "browser_obj",
        "title": "Browser Object",
        "name": "browser_obj",
        "tip": "",
        "value": [
          {
            "type": "str",
            "value": ""
          }
        ],
        "required": true
      },
      {
        "types": "WebPick",
        "formType": {
          "type": "PICK",
          "params": {
            "use": "WebPick"
          }
        },
        "key": "picture_pick",
        "title": "背景图片对象",
        "name": "picture_pick",
        "tip": "",
        "required": true,
        "noInput": true
      },
      {
        "types": "WebPick",
        "formType": {
          "type": "PICK",
          "params": {
            "use": "WebPick"
          }
        },
        "key": "slider_pick",
        "title": "拾取滑块对象",
        "name": "slider_pick",
        "tip": "",
        "required": true,
        "noInput": true
      },
      {
        "types": "Bool",
        "formType": {
          "type": "SWITCH",
          "params": {}
        },
        "key": "unmatched_flag",
        "title": "YesNo为变速Verification Code",
        "name": "unmatched_flag",
        "tip": "",
        "options": [
          {
            "label": "Yes",
            "value": true
          },
          {
            "label": "No",
            "value": false
          }
        ],
        "default": false,
        "required": true
      },
      {
        "types": "WebPick",
        "formType": {
          "type": "PICK",
          "params": {
            "use": "WebPick"
          }
        },
        "key": "move_pic_pick",
        "title": "拾取滑动图片对象",
        "name": "move_pic_pick",
        "tip": "",
        "dynamics": [
          {
            "key": "$this.move_pic_pick.show",
            "expression": "return  $this.unmatched_flag.value == true"
          }
        ],
        "required": true,
        "noInput": true
      },
      {
        "types": "Int",
        "formType": {
          "type": "INPUT_VARIABLE_PYTHON"
        },
        "key": "mini_step",
        "title": "微调步长（px）",
        "name": "mini_step",
        "tip": "",
        "default": 5,
        "value": [
          {
            "type": "str",
            "value": ""
          }
        ],
        "dynamics": [
          {
            "key": "$this.mini_step.show",
            "expression": "return  $this.unmatched_flag.value == true"
          }
        ],
        "required": true
      }
    ],
    "outputList": [
      {
        "types": "Int",
        "formType": {
          "type": "RESULT"
        },
        "key": "drag_distance",
        "title": "移动距离",
        "tip": ""
      }
    ],
    "icon": "captcha-slider",
    "helpManual": ""
  },
  "VerifyCode.click_code": {
