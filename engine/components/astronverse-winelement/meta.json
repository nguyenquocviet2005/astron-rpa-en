        "name": "text",
        "tip": "",
        "default": "",
        "value": [
          {
            "type": "str",
            "value": ""
          }
        ],
        "dynamics": [
          {
            "key": "$this.text.show",
            "expression": "return $this.input_type.value == 'keyboard'"
          }
        ],
        "required": true
