  "WinEle.input_text_element": {
    "key": "WinEle.input_text_element",
    "title": "填写Input框（桌面）",
    "version": "1.0.0",
    "src": "astronverse.winelement.winele.WinEle().input_text_element",
    "comment": "填写Input框 @{pick} ，并设置Input Type为 @{input_type} ，Input Content为 @{text}",
    "inputList": [
      {
        "types": "WinPick",
        "formType": {
          "type": "PICK",
          "params": {
            "use": "ELEMENT"
          }
        },
        "key": "pick",
        "title": "Pick Element",
        "name": "pick",
        "tip": "",
        "required": true,
        "noInput": true
      },
      {
        "types": "ElementInputType",
        "formType": {
          "type": "RADIO"
        },
        "key": "input_type",
        "title": "Input Type",
        "name": "input_type",
        "tip": "",
        "options": [
          {
            "label": "Keyboard Input",
            "value": "keyboard"
          },
          {
            "label": "ClipboardInput",
            "value": "clipboard"
          }
        ],
        "default": "keyboard",
        "required": true
      },
      {
        "types": "Str",
        "formType": {
          "type": "INPUT_VARIABLE_PYTHON"
        },
        "key": "text",
