          {
            "label": "Yes",
            "value": true
          },
          {
            "label": "No",
            "value": false
          }
        ],
        "default": true,
        "required": true
      },
      {
        "types": "Float",
        "formType": {
          "type": "INPUT_VARIABLE_PYTHON"
        },
        "key": "wait_time",
