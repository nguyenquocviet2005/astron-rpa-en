        "tip": "",
        "default": [
          "",
          ""
        ],
        "dynamics": [
          {
            "key": "$this.default_time_range.show",
            "expression": "return $this.time_type.value == 'time_range'"
          },
          {
            "key": "$this.default_time.formType.params.format",
            "expression": "return $this.time_format.value"
          }
        ],
        "required": false
      },
      {
