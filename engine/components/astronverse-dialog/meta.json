{
  "Dialog.message_box": {
    "key": "Dialog.message_box",
    "title": "消息提示框",
    "version": "1.0.0",
    "src": "astronverse.dialog.dialog.Dialog().message_box",
    "comment": "",
    "inputList": [
      {
        "types": "Str",
        "formType": {
          "type": "INPUT"
        },
        "key": "box_title",
        "title": "Dialog标题",
        "name": "box_title",
        "tip": "用户Custom的Dialog上方的显示标题，限制标题内容字符数量为50个字符。不Input则默认标题为“消息提示框”",
        "default": "消息提示框",
        "required": false,
        "limitLength": [
          -1,
          50
        ]
      },
      {
        "types": "MessageType",
        "formType": {
          "type": "RADIO"
        },
        "key": "message_type",
        "title": "消息类型",
        "name": "message_type",
        "tip": "可选消息类型",
        "options": [
          {
            "label": "信息",
            "value": "message"
          },
          {
            "label": "警告",
            "value": "warning"
          },
          {
            "label": "问题",
            "value": "question"
          },
          {
            "label": "错误",
            "value": "error"
          }
        ],
        "default": "message",
        "required": true
      },
      {
        "types": "Str",
        "formType": {
          "type": "INPUT_VARIABLE_PYTHON"
        },
        "key": "message_content",
        "title": "消息内容",
        "name": "message_content",
        "tip": "用户Custom的Dialog内的显示内容，限制内容字符数量为120个字符",
        "default": "",
        "required": true,
        "limitLength": [
          -1,
          120
        ]
      },
      {
        "types": "ButtonType",
        "formType": {
          "type": "SELECT"
        },
        "key": "button_type",
        "title": "展示按钮",
        "name": "button_type",
        "tip": "消息框可展示给用户进行选择的按钮，只允许切换，不允许空选状态",
        "options": [
          {
            "label": "确认",
            "value": "confirm"
          },
          {
            "label": "确认-取消",
            "value": "confirm_cancel"
          },
          {
            "label": "Yes-No",
            "value": "yes_no"
          },
          {
            "label": "Yes-No-取消",
            "value": "yes_no_cancel"
          }
        ],
        "default": "confirm",
        "required": true
      },
      {
        "types": "Bool",
        "formType": {
          "type": "SWITCH"
        },
        "key": "auto_check",
        "title": "None操作自动关闭Dialog",
        "name": "auto_check",
        "tip": "",
        "options": [
          {
            "label": "Yes",
            "value": true
          },
          {
            "label": "No",
            "value": false
          }
        ],
        "default": false,
        "level": "advanced",
        "required": false
      },
      {
        "types": "Int",
        "formType": {
          "type": "INPUT_VARIABLE_PYTHON"
        },
        "key": "wait_time",
        "title": "None操作超时等待（秒）",
        "name": "wait_time",
        "tip": "设置超时等待时间，未识别到用户鼠标移动行为超过该时间则选中默认按钮，默认为60秒",
        "value": [
          {
            "type": "str",
            "value": ""
          }
        ],
        "level": "advanced",
        "dynamics": [
          {
            "key": "$this.wait_time.show",
            "expression": "return $this.auto_check.value == true"
          }
        ],
        "required": false
      },
      {
        "types": "DefaultButtonC",
        "formType": {
          "type": "SELECT"
        },
        "key": "default_button_c",
        "title": "超时默认选中按钮",
        "name": "default_button_c",
        "tip": "",
        "options": [
          {
            "label": "确认",
            "value": "confirm"
          }
        ],
        "default": "confirm",
        "level": "advanced",
        "dynamics": [
          {
            "key": "$this.default_button_c.show",
            "expression": "return $this.auto_check.value == true && $this.button_type.value == 'confirm'"
          }
        ],
        "required": true
      },
      {
        "types": "DefaultButtonCN",
        "formType": {
          "type": "SELECT"
        },
        "key": "default_button_cn",
        "title": "超时默认选中按钮",
        "name": "default_button_cn",
        "tip": "",
        "options": [
          {
            "label": "确认",
            "value": "confirm"
          },
          {
            "label": "取消",
            "value": "cancel"
          }
        ],
        "default": "confirm",
        "level": "advanced",
        "dynamics": [
          {
            "key": "$this.default_button_cn.show",
            "expression": "return $this.auto_check.value == true && $this.button_type.value == 'confirm_cancel'"
          }
        ],
        "required": true
      },
      {
        "types": "DefaultButtonY",
        "formType": {
          "type": "SELECT"
        },
        "key": "default_button_y",
        "title": "超时默认选中按钮",
        "name": "default_button_y",
        "tip": "",
        "options": [
          {
            "label": "Yes",
            "value": "yes"
          },
          {
            "label": "No",
            "value": "no"
          }
        ],
        "default": "yes",
        "level": "advanced",
        "dynamics": [
          {
            "key": "$this.default_button_y.show",
            "expression": "return $this.auto_check.value == true && $this.button_type.value == 'yes_no'"
          }
        ],
        "required": true
      },
      {
        "types": "DefaultButtonYN",
        "formType": {
          "type": "SELECT"
        },
        "key": "default_button_yn",
        "title": "超时默认选中按钮",
        "name": "default_button_yn",
        "tip": "",
        "options": [
          {
            "label": "Yes",
            "value": "yes"
          },
          {
            "label": "No",
            "value": "no"
          },
          {
            "label": "取消",
            "value": "cancel"
          }
        ],
        "default": "yes",
        "level": "advanced",
        "dynamics": [
          {
            "key": "$this.default_button_yn.show",
            "expression": "return $this.auto_check.value == true && $this.button_type.value == 'yes_no_cancel'"
          }
        ],
        "required": true
      },
      {
        "types": "Any",
        "formType": {
          "type": "MODALBUTTON"
        },
        "key": "preview_button",
        "title": "预览",
        "name": "preview_button",
        "tip": "",
        "required": false
      }
    ],
    "outputList": [
      {
        "types": "Str",
        "formType": {
          "type": "RESULT"
        },
        "key": "result_button",
        "title": "保存用户选择的按钮至",
        "tip": ""
      }
    ],
    "icon": "message-dialog",
    "helpManual": "打开消息提示对话框，可设定消息提示的内容和标题。"
  },
