        "default": "time",
        "required": false
      },
      {
        "types": "TimeFormat",
        "formType": {
          "type": "SELECT"
        },
        "key": "time_format",
        "title": "时间格式",
        "name": "time_format",
        "tip": "",
        "options": [
          {
            "label": "年-月-日",
